{
  "name": "amo-widget-builder",
  "version": "0.0.11",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "yarn run clean && tsc --build",
    "build:watch": "nodemon --exec yarn run build",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "yarn run format && yarn run build"
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^9.0.9",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.36",
    "@types/uniqid": "^5.3.0",
    "husky": "^5.2.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "consola": "^2.15.3",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.3",
    "jszip": "^3.6.0",
    "lodash": "^4.17.21",
    "uniqid": "^5.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run format:check"
    }
  }
}
